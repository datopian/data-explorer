{
  "name": "@datopian/data-explorer",
  "version": "0.4.15",
  "private": false,
  "main": "/dist/AppWithProvider.js",
  "module": "/dist/AppWithProvider.js",
  "dependencies": {
    "@datopian/chart-builder": "^0.1.6",
    "@datopian/datapackage-views-js": "git+https://github.com/datopian/datapackage-views-js.git",
    "@datopian/datastore-query-builder": "git+https://github.com/datopian/datastore-query-builder.git#feature/tools-integration",
    "@datopian/map-builder": "^0.1.9",
    "@heroicons/react": "^2.1.3",
    "@tanstack/react-table": "^8.15.0",
    "data.js": "^0.12.11",
    "datapackage-render": "git+https://github.com/frictionlessdata/datapackage-render-js.git",
    "i18next": "^23.15.1",
    "i18next-browser-languagedetector": "^8.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.2",
    "react-loader": "^2.4.5",
    "react-paginate": "^8.2.0",
    "react-redux": "^9.1.2",
    "react-scripts": "5.0.1",
    "react-tabs-redux": "^4.0.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "assert": "^2.1.0",
    "autoprefixer": "^10.4.20",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "cypress": "^14.5.3",
    "eslint-config-react-app": "^7.0.1",
    "path-browserify": "^1.0.1",
    "postcss": "^8.0.0",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^12.0.1",
    "process": "^0.11.10",
    "react-app-rewired": "^2.2.1",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.4.10",
    "url": "^0.11.4",
    "util": "^0.12.5"
  },
  "scripts": {
    "css": "postcss src/css/index.css -o src/App.css",
    "start": "NODE_OPTIONS=\"--openssl-legacy-provider\" SKIP_PREFLIGHT_CHECK=true react-app-rewired start",
    "build": "set GENERATE_SOURCEMAP=false && yarn css && react-scripts build",
    "build:package": "rm -rf dist/ && mkdir dist && NODE_ENV=production npx babel src/ -d dist/ --copy-files",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --spec 'cypress/e2e/**/*-dev.cy.js'",
    "cypress:open:specs": "cypress open",
    "cypress:run:specs": "cypress run --spec 'cypress/e2e/**/*-spec-dev.cy.js'"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "14"
          }
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-optional-chaining",
      "@babel/plugin-proposal-nullish-coalescing-operator"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "sideEffects": [
    "./src/i18n.js"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "resolutions": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
