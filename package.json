{
  "name": "data-explorer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "chart-builder": "git+https://github.com/datopian/chart-builder.git",
    "data.js": "^0.12.10",
    "datapackage-render": "git+https://github.com/frictionlessdata/datapackage-render-js.git",
    "datapackage-views-js": "git+https://github.com/datopian/datapackage-views-js.git#refactor/data-views",
    "datastore-query-builder": "git+https://github.com/datopian/datastore-query-builder.git#feature/package",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-loader": "^2.4.5",
    "react-redux": "^7.1.0",
    "react-scripts": "3.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ]
  },
  "nohoist": [
    "**/babel-preset-react-app/@babel/runtime"
  ],
  "devDependencies": {
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.5.5",
    "autoprefixer": "^9.6.1",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "redux-devtools-extension": "^2.13.8",
    "tailwindcss": "^1.1.1"
  },
  "scripts": {
    "css": "postcss src/css/index.css -o src/App.css",
    "start": "PORT=5000 react-scripts start",
    "build": "yarn css && react-scripts build",
    "test": "NODE_PATH=. react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
